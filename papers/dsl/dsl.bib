	issn = {0167-6423},
	journal = {Science of Computer Programming},
	keywords = {Ontology},
	note = {Special Issue on Knowledge-based Software Engineering},
	pages = {100-127},
	title = {Making explicit domain knowledge in formal system development},
	url = {https://www.sciencedirect.com/science/article/pii/S0167642315004153},
