	issue_date = {Jan. 2000},
	journal = {SIGPLAN Not.},
	keywords = {DSL, Category Theory, Monad},
	month = {dec},
	number = {1},
	numpages = {14},
	pages = {81--94},
	publisher = {Association for Computing Machinery},
	title = {DSL Implementation Using Staging and Monads},
